{
  "name": "akbsit/nova-card-log",
  "description": "Cleaning application log files from laravel nova dashboard card.",
  "keywords": [
    "laravel",
    "nova",
    "files",
    "logs"
  ],
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "laravel/framework": "^9.0|^10.0|^11.0",
    "laravel/nova": "^3.0|^4.0"
  },
  "autoload": {
    "psr-4": {
      "Akbsit\\NovaCardLog\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Akbsit\\NovaCardLog\\CardServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
